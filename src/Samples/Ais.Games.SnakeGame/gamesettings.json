{
  "GameEngineModules": {
    "Default": [
      "Ais.GameEngine.Extensions.SignalBus"
    ],
    "main": [
      "Ais.GameEngine.Extensions.Commands",
      "Ais.GameEngine.Extensions.Ecs"
    ]
  },
  "GameEngineSettings": {
    "TargetFrameRate": 30,
    "FixedDeltaTime": 0.013
  },
  "GameWindowSettings": {
    "Title": "Змейка",
    "Width": 30,
    "Height": 30
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "Configure": [
            {
              "Name": "File",
              "Args": {
                "path": "./Logs/app-.log",
                "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}",
                "rollingInterval": "Day",
                "fileSizeLimitBytes": 10485760,
                "retainedFileCountLimit": 30,
                "rollOnFileSizeLimit": false
              }
            }
          ]
        }
      }
    ],
    "Enrich": [
      "WithMachineName",
      "WithThreadId",
      "FromLogContext"
    ],
    "Using": [
      "Serilog.Sinks.Async",
      "Serilog.Sinks.File"
    ],
    "Properties": {
      "Application": "Snake"
    }
  }
}
